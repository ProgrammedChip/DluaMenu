local InsertService = game:GetService("InsertService")

local modelId = 123456 -- Replace with the ID of the model you want to insert

local LocalScript = script.Parent
local model = nil

-- This function will run when the LocalScript is activated
function onActivated()
  model = InsertService:LoadAsset(modelId)
  game:GetObjects("Workspace")[1]:Insert(model)
end

-- Connect the onActivated function to the Activated event
LocalScript.Activated:Connect(onActivated)
